<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Flappy_Miau.Views"
    x:Class="Flappy_Miau.AppShell"
    Shell.FlyoutBehavior="Disabled"
    Title="Flappy Miau">
    <ShellContent
        ContentTemplate="{DataTemplate views:MainMenuPage}"
        Route="MainMenuPage" />
    <ShellContent
        ContentTemplate="{DataTemplate views:GamePage}"
        Route="GamePage" />
    <ShellContent
        ContentTemplate="{DataTemplate views:HighScorePage}"
        Route="HighScorePage" />
</Shell>